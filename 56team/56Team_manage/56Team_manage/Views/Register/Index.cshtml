@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>注册</title>
    <script type="text/javascript" src="~/Scripts/jquery-1.8.2.js">
        
    </script>
</head>
<body>

    <table align="center">
        <tr><td>@Html.ActionLink("返回登陆","Index","Login")</td></tr>
        <tr><td>56工作室注册</td></tr>
        @using (Html.BeginForm("Index","Register",FormMethod.Post))
        { 
            <tr><td>请输入用户名：<input type="text" name="userName" /> </td></tr>
            <tr><td>密码：<input type="password" name="userPwd" id="pwdFirst" /></td></tr>
            <tr><td>请再输入一次密码:<input type="password" id="pwdSconed" name="userPedSconed"/></td></tr>
            <tr><td>请输入验证码:<input type="text" name="vcode"><img src="/Vcode/getVcode" /></td></tr>
            <tr><td><input type="submit" value="提交" id="btn"/> </td></tr>
        }
    </table>
           
    
</body>
</html>
